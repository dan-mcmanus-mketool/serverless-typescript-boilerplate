    "test:watch": "jest --watch --coverage",
    "lint": "eslint '*/**/*.{js,ts}'",
    "prettier": "prettier --write '*/**/*.{js,ts}'",
    "docs": "npx @redocly/cli preview-docs docs/openapi/openapi.yml"
  },
