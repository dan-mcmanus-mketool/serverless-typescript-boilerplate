    "serverless-offline": "^12.0.3",
    "serverless-plugin-aws-alerts": "^1.7.5",
    "serverless-prune-plugin": "^2.0.1",
    "serverless-stage-manager": "^1.0.5",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.2",
